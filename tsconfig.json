{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "lib": ["ESNEXT","DOM","DOM.Iterable","WebWorker"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "outDir": "dist"
  },
  "include": [
    "src/**/*.ts"
  ]
}
